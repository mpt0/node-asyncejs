{
	"name": "subcode",
	"version": "2.4.0",
	"description": "A bootstrapped javascript template engine that features compile and runtime control code, custom syntax, compile-time includes, async templates and much more",
	"keywords": [
		"template",
		"engine",
		"compile-time",
		"runtime",
		"control code",
		"custom syntax",
		"async",
		"embedded",
		"resources",
		"javascript",
		"parser",
		"bootstrapped",
		"compiler",
		"extensions"
	],
	"main": "lib",
	"repository": "github:mpt0/node-subcode",
	"author": "Max J. Polster",
	"license": "MIT",
	"scripts": {
		"test": "xo && ava",
		"dev": "ava --verbose --watch"
	},
	"xo": {
		"rules": {
			"no-eval": 0
		}
	},
	"ava": {
		"files": [
			"test/*.js"
		],
		"source": [
			"**/*.{js,json,html}",
			"!test/data/dist/**"
		]
	},
	"devDependencies": {
		"ava": "^0.25.0",
		"fs-extra": "^6.0.1",
		"xo": "^0.21.1"
	},
	"dependencies": {
		"escape-html": "^1.0.3",
		"js-string-escape": "^1.0.1",
		"stringify-object": "^3.2.2"
	}
}
